    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@ngrx/effects": "^14.3.3",
    "@ngrx/store": "^14.3.3",
    "bootstrap": "^5.2.0",
    "cypress": "^12.7.0",
